{
  "name": "nightwatch_playground",
  "version": "1.0.0",
  "description": "Nightwatch playground",
  "main": "index.js",
  "scripts": {
    "nightwatch": "nightwatch -c conf/nightwatch.conf.js --group demo -e firefox",
    "docker": "nightwatch -c conf/nightwatch.conf.js --group demo -e local_docker",
    "cucumber": "mkdirp report && cucumber-js --require ./conf/cucumber.conf.js --require ./step-definitions --format node_modules/cucumber-pretty --format json:report/cucumber_report.json"
  },
  "keywords": [
    "nightwatch",
    "framework",
    "test",
    "playground"
  ],
  "author": "RazvanV",
  "license": "ISC",
  "devDependencies": {
    "chromedriver": "^79.0.0",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.1.0",
    "cucumber-pretty": "^6.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "geckodriver": "^1.19.1",
    "iedriver": "^3.14.1",
    "mkdirp": "^1.0.3",
    "nightwatch": "^1.3.4",
    "nightwatch-api": "^3.0.1",
    "prettier": "1.19.1",
    "selenium-server": "^3.141.59"
  }
}
